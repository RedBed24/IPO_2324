        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid x:Name="pnlDisenoPrincipal" Margin="0,0,0,1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="111*"/>
